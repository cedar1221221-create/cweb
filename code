var oo = 0;

function openVersions() {
  if (oo < 2) {
    document.getElementById('versionss').style.display = 'block';
    oo = oo + 1;
  } if (oo > 1) {
    document.getElementById('versionss').style.display = 'none';
    oo = 0;
  }
}
</script>

